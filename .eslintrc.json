{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": 1,
        "@typescript-eslint/explicit-function-return-type": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0,
        "no-debugger": 1
    },
    "settings": {
        "react": {
            "version": "latest",
            "import/resolver": {
                "typescript": {}
            }
        }
    }
}
